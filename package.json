{
  "name": "eslint-plugin-fatfisz",
  "version": "4.0.1",
  "description": "ESLint config for the FatFisz coding standard",
  "main": "index.js",
  "homepage": "https://github.com/fatfisz/eslint-config-fatfisz",
  "repository": "https://github.com/fatfisz/eslint-config-fatfisz.git",
  "author": {
    "name": "FatFisz",
    "url": "https://github.com/fatfisz"
  },
  "license": "MIT",
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "plugin",
    "styleguide"
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "mocha \"tests/**\"",
    "test:watch": "yarn test --watch",
    "verify:ci": "yarn build && yarn verify:no-changes && yarn lint --max-warnings 0 && yarn test",
    "verify:no-changes": "node ./verifyNoChanges"
  },
  "dependencies": {
    "eslint-config-prettier": "8",
    "eslint-plugin-import": "2.25",
    "eslint-plugin-prettier": "4",
    "globals": "13",
    "prettier": "2"
  },
  "devDependencies": {
    "@types/eslint": "8",
    "@types/estree": "0",
    "@types/node": "17",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "@typescript-eslint/types": "5",
    "eslint": "8",
    "eslint-plugin-react": "7",
    "eslint-plugin-react-hooks": "4",
    "mocha": "9",
    "simple-git": "2",
    "type-fest": "2",
    "typescript": "4"
  },
  "peerDependencies": {
    "eslint": "8",
    "eslint-config-prettier": "8",
    "eslint-plugin-import": "2.25",
    "eslint-plugin-prettier": "4",
    "prettier": "2"
  }
}
